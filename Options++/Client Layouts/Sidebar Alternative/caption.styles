		}
		
		FrameMaximizeButton:active [$OSX]
		{
			image="graphics/osx_max_down"
			image="graphics/osx_max_down_new" [$OSX1010]
		}

		
		FrameRestoreButton:hover [$OSX]
		{
			image="graphics/osx_max_hov"
			image="graphics/osx_max_hov_new" [$OSX1010]
		}
