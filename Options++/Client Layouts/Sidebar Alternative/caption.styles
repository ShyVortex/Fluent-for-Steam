			render_bg {}
			bgcolor="none"
			image="graphics/osx_max_hov"
			image="graphics/osx_max_hov_new" [$OSX1010]
		}
		
		FrameRestoreButton:active [$OSX]
		{
			image="graphics/osx_max_down"
			image="graphics/osx_max_down_new" [$OSX1010]
		}
		
		FrameCloseButton [$OSX]
		{
			render_bg{}
			image="graphics/osx_win_dis"
			image="graphics/osx_win_dis_new_dark" [$OSX1010]
		}
		
		
		FrameCloseButton:hover [$OSX]
		{
			image="graphics/osx_close_hov"
			image="graphics/osx_close_hov_new" [$OSX1010]
		}
		FrameCloseButton:framefocus [$OSX]
		{
			render_bg {}
			bgcolor="none"
			image="graphics/osx_close_def"
			image="graphics/osx_close_def_new" [$OSX1010]
		}
		FrameCloseButton:framefocus:hover [$OSX]
		{
			render_bg {}
			bgcolor="none"
			image="x0+6,y0+8,x1,y1graphics/osx_close_hov"
			image="graphics/osx_close_hov_new" [$OSX1010]
		}
		
		FrameCloseButton:active [$OSX]
		{
			image="graphics/osx_close_down"
			image="graphics/osx_close_down_new" [$OSX1010]
		}
		
		FrameCloseButton:disabled [$OSX]
		{
			render_bg{}
			image="graphics/osx_win_dis"
			image="graphics/osx_win_dis_new_dark" [$OSX1010]
		}

		//Semi Style
		"ToolWindow FrameMinimizeButton"
		{
			inset="4 7 0 0"
			render_bg{}
			image="graphics/fluent/icons/caption/sm/chat_min_sm"
		}
		"ToolWindow FrameMinimizeButton:hover"
		{
			bgcolor="none"
		}
		"ToolWindow FrameMinimizeButton:active"
		{
			bgcolor="none"
		}

