"caption.styles"
{
	styles
	{
		//Default Style
		///////////////////////BOTON DE MINIMIZAR////////////////////////////////////////////
		FrameMinimizeButton [$WIN32||$WINDOWS||$LINUX]
		{
			
			render_bg {
				1 = "image(x0, y0, x0, y0, caption/minimizar"
			}
		}
		FrameMinimizeButton:hover [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor=white07
			
			
		}
		FrameMinimizeButton:active [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor=white05
			
		}
		////////////////////////////BOTON MAXIMIZAR///////////////////////////////////////	
		FrameMaximizeButton [$WIN32||$WINDOWS||$LINUX]
		{
			render
			{
				0="image(x0+0,y0+0,x1,y1,caption/maximizar)"
			}
		}
		FrameMaximizeButton:hover [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor=white07
			
		}
		FrameMaximizeButton:active [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor="white05"
			
		}
        /////////////////////////////////////boton restaurar////////////////////////////////////////
        FrameRestoreButton [$WIN32||$WINDOWS||$LINUX]
		{
			
			render
			{
				0="image(x0+0,y0+0,x1,y1,caption/restore)"
			}
		}
		FrameRestoreButton [$OSX]
		{
			render_bg{}
			image="graphics/osx_win_dis"
			image="graphics/osx_win_dis_new_dark" [$OSX1010]
		}
		FrameRestoreButton:hover [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor=white07
			render
			{
				0="image(x0+0,y0+0,x1,y1,caption/restore)"
			}
		}
		FrameRestoreButton:active [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor=white05
			render
			{
				0="image(x0+0,y0+0,x1,y1,caption/restore)"
			}
		}
		///////////////////////////////////boton close/////////////////////////////////////////////////////////////
        FrameCloseButton [$WIN32||$WINDOWS||$LINUX]
		{
			render
			{
				0="image(x0,y0,x1,y1,caption/close)"
			}
		}
		FrameCloseButton:hover [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor=Red75
			
		}
		FrameCloseButton:active [$WIN32||$WINDOWS||$LINUX]
		{
			bgcolor=RED45
		
		}
		FrameCloseButton:disabled [$WIN32||$WINDOWS||$LINUX]
		{
			render_bg{}
			image="caption/cerrarnormal"
		}
         //////////////////////////////////////////////////////////////////////////////////////////////////////////////
		FrameMinimizeButton [$OSX]
