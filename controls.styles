		ComboBox:disabled
		{
			textcolor="none"
			render
			{
				1="fill(x0,y0,x1,y0+1,none)"  // top
				2="fill(x0,y1-1,x1,y1+2,focus)"  // bottom
				3="fill(x0,y0+1,x0+1,y1-1,none)"  // left
				4="fill(x1-1,y0+1,x1,y1-1,none)"  // right
			}
		}
	
	

		ComboBoxButton
		{
			bgcolor="none"
			textcolor="none"
			render_bg
			{
				1="image( x8, y0+0, y0, x0-0, combobox/down )"
			}

		}
		ComboBoxButton:hover
		{
			textcolor="none"
			bgcolor="none"
			render_bg
			{
				1="image( x0, y0+0, y0, x0-0, combobox/down_h )"
			}


		}
	

	
		
		"PageDragFrame "
		{
			font-style=""
			font-size=14
		}

		ScrollBarButton.up
		{
		
			bgcolor="none"
			render_bg{
				0="image(x0,y0,x1,y1,graphics/scrollbar/up)"
			}
			
			
		}
		

		ScrollBarButton.down
		{
			
			bgcolor="none"
			
			render_bg{
				0="image(x0,y0,x1,y1,graphics/scrollbar/down)"
			}
			
		}
		
		ScrollBarButton.left
		{
			bgcolor="none"
			render_bg{}
