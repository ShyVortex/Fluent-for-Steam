                ////////////medio de parte izquierda///////////////////////////////////////////////////
				3="image_tiled(x0-3,y0+4,x0+2,y1-4, materiales/b/activo/mi)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
