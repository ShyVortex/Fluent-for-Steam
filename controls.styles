		"SliderHoriz"  //horizontal scrollbar thumb
		{
			bgcolor=scroll
		}
		"SliderHoriz:hover"
		{
			bgcolor=scroll_a
			render_bg
			{
				0="fill( x0, y0, x1, y1, white05 )"
			}
		}
		"SliderHoriz:selected"
