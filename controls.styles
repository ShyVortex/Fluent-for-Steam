		}

		ScrollBarHandle  //////////////barra de scroll vertical////////////////////
		{
			bgcolor="none"
			render_bg {
				0="fill(x0+3,y0+3,x1-3,y1-3,scrollbardark)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
				5="image(x0+3,y0,x0,y0+3, materiales/scrollbar/ti)"
				/////////////////////////esquina derecha superior///////////////////////////////////////////////
				7="image(x1-6,y0-0,x1,y0+5, materiales/scrollbar/td)"	
           					
				///////////////////esquina izquierda inferior/////////////////////////////
